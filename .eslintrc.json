{
    "extends": [
        "@rocketseat/eslint-config/node",
        "plugin:prettier/recommended"
    ],
    "env": {
        "es6": true,
        "node": true
    },
    "rules": {
        "prettier/prettier": "error",
        "camelcase": "off",
        "no-useless-constructor": "off"
    },
    "plugins": [
        "prettier"
    ]
}